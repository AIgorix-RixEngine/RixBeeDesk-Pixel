___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const encodeUri = require('encodeUri');
const accountId = data.accountId;
const conversionType = data.conversionType;
const currency = data.currency;
const value = data.value;
const total_value1 = data.total_value1;
const total_value2 = data.total_value2;
const total_conversions = data.total_conversions;
const ifUseGtm = 1;
const params = {
  name:conversionType,
  currency:currency,
  total_conversions:total_conversions,
  ifUseGtm:ifUseGtm
};
// 若没填value,就不传total_value
if(value){
  params.value = value;
  params.total_value=total_value1;
}
// 加载adv脚本
const url = 'https://crs.rixbeedesk.com/wrapper/' + encodeUri(accountId) + '.js';
injectScript(url, data.gtmOnsuccess, data.gtmOnFailure, url);
const clicker = copyFromWindow('eleForClicker');
const onSuccess = () => {
  const clicker = copyFromWindow('eleForClicker');
  log('callback loaded',conversionType,clicker);
  if (clicker) {
    clicker.track(params);
  }
};
if (!clicker) {
  const cb_js = 'https://crs.rixbeedesk.com/callback.js';
  injectScript(cb_js, onSuccess, data.gtmOnFailure, cb_js);
} else if (clicker && data.conversionType !== 'pageview') {
  clicker.track(params);
}


